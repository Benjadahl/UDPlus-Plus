{
  "manifest_version": 2,

  "name": "Uddata++",
  "description": "An exstesion that makes the experience of using uddata+ better",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches":[
        "http://www.uddataplus.dk/*",
        "https://www.uddataplus.dk/*"
      ],
      "js": ["jquery-3.1.0.min.js","content.js"],
      "run_at": "document_end"
    }
  ]
}
