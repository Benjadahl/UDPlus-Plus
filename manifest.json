{
  "manifest_version": 2,

  "name": "Uddata++",
  "description": "An extension that makes the experience of using uddata+ better",
  "version": "0.1",

  "permissions": [
    "activeTab",
		"storage"
  ],
  "content_scripts": [
    {
      "matches":[
        "http://www.uddataplus.dk/*",
        "https://www.uddataplus.dk/*"
      ],
      "js": ["jquery-3.1.0.min.js","UD++Functions.js","main.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["UddataLogo.png", "script.js", "settings.html"]
}
